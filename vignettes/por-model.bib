@article{Wang-L-2016-biom-ph-spline,
Abstract = {The proportional hazards model (PH) is currently the most popular regression model for analyzing time-toevent data. Despite its popularity, the analysis of interval-censored data under the PH model can be challenging using many available techniques. This article presents a new method for analyzing interval-censored data under the PH model. The proposed approach uses a monotone spline representation to approximate the unknown nondecreasing cumulative baseline hazard function. Formulating the PH model in this fashion results in a finite number of parameters to estimate while maintaining substantial modeling flexibility. A novel expectation-maximization (EM) algorithm is developed for finding the maximum likelihood estimates of the parameters. The derivation of the EM algorithm relies on a two-stage data augmentation involving latent Poisson random variables. The resulting algorithm is easy to implement, robust to initialization, enjoys quick convergence, and provides closed-form varianc},
Author = {Wang, Lianming and McMahan, Christopher S. and Hudgens, Michael G. and Qureshi, Zaina P.},
ISSN = {0006341X},
Journal = {Biometrics},
Keywords = {PROPORTIONAL hazards models, CONFIDENCE intervals, REGRESSION analysis, RANDOMIZED controlled trials, EM algorithm, Interval-censored data, Interval-censored data, Latent Poisson random variables, Monotone splines, Proportional hazards model, NATIONAL Cancer Institute (U.S.)},
Number = {1},
Pages = {222--231},
Title = {A Flexible, Computationally Efficient Method for Fitting the Proportional Hazards Model to Interval-Censored Data.},
Volume = {72},
Year = {2016},
}
@article{Wei-etal-1990-bka,
 ISSN = {00063444},
 URL = {http://www.jstor.org/stable/2337107},
 abstract = {Recently linear rank statistics with censored data have been used as the estimating functions for the regression parameters in the linear model with an unspecified error distribution. The resulting rank estimators are consistent and asymptotically normal. However, the asymptotic variances of these estimators are complicated and are difficult to estimate well with censored data. In this paper, we propose some simple methods for making inference about a subset of the regression coefficients while regarding others as nuisance parameters. A lack-of-fit test for the linear model is also presented. The proposed procedures are illustrated with an example.},
 author = {L. J. Wei and Z. Ying and D. Y. Lin},
 journal = {Biometrika},
 number = {4},
 pages = {845--851},
 publisher = {[Oxford University Press, Biometrika Trust]},
 title = {Linear Regression Analysis of Censored Survival Data Based on Rank Tests},
 volume = {77},
 year = {1990}
}
